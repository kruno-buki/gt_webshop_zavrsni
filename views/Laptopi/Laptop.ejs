                <p>vrijednost: <span id="range-cijena"></span></p>
            </div>
            <div id="btnContainer">
                <button class="btn active" onclick="prikaziSve()">prikaži sve</button>
                <button class="btn" onclick="prikazi('lenovo')">lenovo</button>
                <button class="btn" onclick="prikazi('dell')">dell</button>
                <button class="btn" onclick="prikazi('apple')">apple</button>
                <button class="btn" onclick="prikazi('acer')">acer</button>
                <button class="btn" onclick="prikazi('hp')">hp</button>
            </div>

            <div class="provjera processor">
                <h3>procesor</h3>
                <div>
                    <label for="intel">intel</label>
                    <input type="checkbox" id="intel-procesor" name="procesori" class="chk">

                    <label for="amd-procesor">amd</label>
                    <input type="checkbox" id="amd-procesor" name="procesori" class="chk">
                </div>
            </div>
            <div class="provjera grafička">
                <h3>grafička</h3>
                <div>
                    <label for="integriranaKartica">integrirana</label>
                    <input type="checkbox" id="integriranaKartica" name="procesor" class="chk">

                    <label for="zasebnaKartica">zasebna</label>
                    <input type="checkbox" id="zasebnaKartica" name="procesor" class="chk">
                </div>
            </div>
            <div class="provjera ekran">
                <h3>zaslon</h3>
                <div>
                    <label for="14incha">14"</label>
                    <input type="checkbox" id="14incha" name="procesor" class="chk">

                    <label for="15incha">15"6'</label>
                    <input type="checkbox" id="15incha" name="procesor" class="chk">

                    <label for="17incha">17"3'</label>
                    <input type="checkbox" id="17incha" name="procesor" class="chk">
                </div>
            </div>
            <div class="provjera memorija">
                <h3>radna m.</h3>
                <div>
                    <label for="4gb">4 gb</label>
                    <input type="checkbox" id="mem4gb" name="procesor" class="chk">

                    <label for="8gb">8 gb</label>
                    <input type="checkbox" id="mem8gb" name="procesor" class="chk">

                    <label for="16gb">16 gb</label>
                    <input type="checkbox" id="mem16gb" name="procesor" class="chk">
                </div>
            </div>

            <div class="provjera disk">
                <h3>pohrana</h3>
                <div>
                    <label for="250gb">250 gb</label>
                    <input type="checkbox" id="d256" name="procesor" class="chk">

                    <label for="500gb">500 gb</label>
                    <input type="checkbox" id="d512" name="procesor" class="chk">
                </div>
            </div>
        </section>
        
